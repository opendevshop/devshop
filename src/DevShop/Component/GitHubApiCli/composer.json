{
  "name": "devshop/github-api-cli",
  "type": "library",
  "description": "Simple console wrapper for GitHub API",
  "keywords": [],
  "homepage": "https://getdevshop.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Jon Pugh",
      "email": "jon@thinkdrop.net"
    },
    {
      "name": "DevShop Community",
      "homepage": "https://github.com/opendevshop/devshop/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1||^8.0",
    "consolidation/robo": "^2",
    "knplabs/github-api": "3.9.x-dev",
    "devshop/composer-common": "1.x-dev",
    "guzzlehttp/guzzle": "7.5.x-dev"
  },
  "repositories": {
    "devshop/composer-common": {
      "type": "path",
      "url": "../ComposerCommon"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "DevShop\\Component\\GitHubApiCli\\": ""
    }
  },
  "bin": [
    "github"
  ],
  "minimum-stability": "dev"
