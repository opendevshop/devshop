{
  "name": "devshop/deploy",
  "type": "library",
  "description": "Simplified tooling for managing your apps deployment in code.",
  "keywords": [],
  "homepage": "https://getdevshop.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Jon Pugh",
      "email": "jon@thinkdrop.net"
    },
    {
      "name": "DevShop Community",
      "homepage": "https://github.com/opendevshop/devshop/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1||^8.0",
    "consolidation/robo": "^1.0",
    "devshop/git-traits": "1.x-dev",
    "eloquent/composer-config-reader": "^2.1"
  },
  "require-dev": {
